#include <stdio.h>  
#include<string.h>
int main()  
{  
    int i, length = 0;  
    char str[] = "aDiTyA";   
    length = sizeof(str)/sizeof(str[0]) ;
    for(i = 0; i < length; i++)
    {
        if(upper(str[i])
        {   
            str[i] = lower(str[i]);  
        } 
        else if(lower(str[i]))
        {  
            str[i] = upper(str[i]);  
        }  
    }  
    printf("String after case conversion : %s", str);  
    return 0;  
}  
