#include <stdio.h>
int main()
{
  int arr[10],n,i,a, count=0;
  printf("enter size of the array");
  scanf("%d",&n);
  printf("enter array elements");
  for(i=0;i<n;i++)
  {
    scanf("%d",&a);
  }
  for(i=0;i<n;i++)
  {
    if(arr[i]>0)
    {
    arr[count++]=arr[i];
  }
  }
  while(count<n)
  {
  arr[count++]=0;
  }
  for(i=0;i<n;i++)
  {
  printf("%d",arr[i]);
  }
  return 0;
}
